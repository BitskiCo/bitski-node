{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "quotemark": [true, "single", "avoid-escape", "avoid-template"],
      "interface-name": [true, "never-prefix"],
      "max-line-length": {
          "options": [160]
      },
      "indent": [true, "spaces", 2],
      "no-switch-case-fall-through": true,
      "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
